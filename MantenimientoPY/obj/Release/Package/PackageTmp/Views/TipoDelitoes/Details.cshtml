@model MantenimientoPY.Models.TipoDelito

@{
    ViewBag.Title = "Tipo delito - Detalle";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Detalle tipo de delito</h2>

<div>
    <h4>Tipo de delito</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre del delito: 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreTipoDelito)
        </dd>

        <dt>
           Descripción: 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.descripcionTipoDelito)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idTipoDelito }) |
    @Html.ActionLink("Volver a la lista", "Index")
</p>
